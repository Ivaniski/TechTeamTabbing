{
  "manifest_version": 2,

  "name": "Tech Team Tabbing",
  "description": "This extension allows you to tab all day.",
  "version": "1.0",

  "browser_action": {
    "default_icon": "icon.png",
    "default_popup": "popup.html"
  },
  "permissions": [
    "activeTab",
    "storage"
  ],

  "background": {
        "persisten": false,
        "scripts": ["background.js"]
    },

  "commands": {

        "mute_tab": {
            "description": "Mute/Unmute the selected tab",
            "suggested_key": {
                "default": "Ctrl+M"
            }
        },

        "mute_tabs": {
            "description": "Mute/Unmute all tabs",
            "suggested_key": {
                "default": "Shift+Alt+M"
            }
        },

        "mute_unselected_tabs": {
            "description": "Mute/Unmute all tabs outside the current",
            "suggested_key": {
                "default": "Shift+Alt+N"
            }
        },

        "tech_team": {
            "description": "Tech Team! ",
            "suggested_key": {
                "default": "Shift+Alt+L"
            }
        }
        
  }

}
